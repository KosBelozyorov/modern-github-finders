{
  "name": "modern-github-finders",
  "homepage": "https://KosBelozyorov.github.io/modern-github-finders/",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .jsx,.js ./src/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "*.css": [
      "stylelint",
      "git add"
    ],
    "*.js": [
      "eslint",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@mate-academy/eslint-config-react": "0.0.4",
    "@mate-academy/stylelint-config": "0.0.6",
    "eslint": "^6.7.2",
    "gh-pages": "^2.1.1",
    "husky": "^3.1.0"
  }
}
